#!/bin/sh

yum install -y socat
yum install -y tmux

mkdir -p .ssh

echo 'ssh-rsa  Latitude E6230' > .ssh/authorized_keys

echo 'ssh-rsa  ifduyue@gmail.com' >> .ssh/authorized_keys

tmux new-session -d 'socat TCP4-LISTEN:8688,fork TCP4:208.68.36.80:8388'
tmux new-session -d 'socat TCP4-LISTEN:8788,fork TCP4:106.187.39.228:8388'
