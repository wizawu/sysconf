{
  "C_Cpp.default.cppStandard": "gnu++20",
  "Prettier-SQL.SQLFlavourOverride": "mariadb",
  "[css]": {
    "editor.detectIndentation": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.detectIndentation": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.detectIndentation": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.detectIndentation": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[less]": {
    "editor.defaultFormatter": "michelemelluso.code-beautifier",
    "editor.detectIndentation": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnType": true
  },
  "[scss]": {
    "editor.detectIndentation": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.detectIndentation": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.detectIndentation": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "beautify.options": {
    "preserve_newlines": true,
    "selector_separator_newline": false
  },
  "black-formatter.args": [
    "--line-length",
    "120"
  ],
  "debug.console.fontFamily": "Courier Prime Code",
  "debug.console.fontSize": 12,
  "debug.javascript.codelens.npmScripts": "never",
  "editor.fontFamily": "Courier Prime Code, OPPOSans",
  "editor.fontSize": 12,
  "editor.formatOnSave": false,
  "editor.letterSpacing": -0.5,
  "editor.lineHeight": 17,
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": {
    "comments": true,
    "other": true,
    "strings": true
  },
  "editor.rulers": [
    80,
    120
  ],
  "editor.stickyScroll.enabled": false,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "punctuation.definition.comment"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "off",
  "files.defaultLanguage": "json",
  "files.exclude": {
    "**/*.retry": true,
    "**/__pycache__": true,
    "**/node_modules/": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.enabled": false,
  "html.autoClosingTags": false,
  "javascript.autoClosingTags": false,
  "keyboard.dispatch": "keyCode",
  "python.analysis.extraPaths": [
    "./lib"
  ],
  "python.createEnvironment.trigger": "off",
  "python.testing.pytestEnabled": true,
  "redhat.telemetry.enabled": true,
  "terminal.integrated.fontSize": 11,
  "typescript.autoClosingTags": false,
  "vim.cursorStylePerMode.normal": "line",
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": false,
  "window.openFoldersInNewWindow": "on",
  "window.restoreWindows": "none",
  "window.zoomLevel": 1.2,
  "workbench.colorTheme": "Atom Material Theme",
  "workbench.editor.enablePreview": false,
  "workbench.editor.restoreViewState": false,
  "workbench.startupEditor": "none",
  "workbench.tree.indent": 16,
  "yaml.validate": false
}
